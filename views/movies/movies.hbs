   <h1>Ironhack Cinema</h1>
    <h3>Click on the movie to check the showtimes!</h3>
<div class="movies-container">
     {{#each this.allMovies}}
    <div class = "movie-card">
    <img src="{{this.image}}" alt="filmImage" width="200px">
    <p class="movie-title">{{this.title}}</p>
     <p>
        <a href="/movies/{{this._id}}">See more</a>
    </p>

    </div>
    {{/each}}
   
       
</div>